{
  "name": "parking-signs-detection",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start:cluster": "pm2 start ecosystem.config.js --env production",
    "start:cluster:dev": "pm2-dev start ecosystem.config.js",
    "dev": "nodemon src/index.ts",
    "clear:dist": "rm -rf dist",
    "build": "npm run clear:dist && npm run build:tsc && npm run copy:model",
    "build:server": "esbuild src/index.ts --bundle --platform=node --outdir=dist --loader:.bin=binary",
    "build:tsc": "tsc",
    "copy:model": "cp -a src/data/. dist/data",
    "lint": "tsc",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:artillery": "artillery run test/artillery.config.yml"
  },
  "nodemonConfig": {
    "exec": "NODE_ENV=development node -r esbuild-register",
    "ext": "js,json,ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@tensorflow/tfjs-node": "^4.1.0",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "artillery": "^2.0.0-27",
    "cors": "^2.8.5",
    "esbuild": "^0.15.15",
    "esbuild-node-tsc": "^2.0.4",
    "esbuild-register": "^3.4.1",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
